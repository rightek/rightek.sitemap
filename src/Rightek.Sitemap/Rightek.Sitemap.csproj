<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Mehdi Dehghani</Authors>
    <Company>Rightek</Company>
    <Copyright>Rightek</Copyright>

    <Version>1.0.0</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>

    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>

    <RepositoryUrl>https://github.com/rightek/rightek.sitemap</RepositoryUrl>
    <PackageProjectUrl>https://github.com/rightek/rightek.sitemap</PackageProjectUrl>
    <PackageReadmeFile>https://github.com/rightek/rightek.sitemap/blob/master/README.md</PackageReadmeFile>

    <PackageId>Rightek.Sitemap</PackageId>
    <Title>Rightek.Sitemap</Title>
    <Description>Simple sitemap generator. Image and SiteMapIndex are supported.</Description>
    <PackageTags>netstandard netcore google sitemap sitemapindex</PackageTags>
  </PropertyGroup>
</Project>